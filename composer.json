{
  "name": "rebuy/ean-isbn-library",
  "description": "A PHP library for EAN and ISBN validation and conversion",
  "license": "MIT",
  "keywords": ["ean", "isbn", "validation", "conversion", "parser", "parsing"],
  "homepage": "https://github.com/rebuy-de/ean-isbn-library",
  "authors": [
    {
      "name": "reBuy reCommerce GmbH",
      "homepage": "https://github.com/rebuy-de/ean-isbn-library/contributors"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rebuy\\EanIsbn\\": ["src/"]
    }
  },
  "autoload-dev": {
